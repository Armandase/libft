#include <string.h>
 42 #include <stdio.h>
 43 #include <unistd.h>
 44 int main(void)
 45 {   
 46     char    str[] = "Chaine de caratere.";
 47     char    dest[8]; 
 48     ft_memmove(dest, str, 4);
 49     char    str1[] = "Chaine de caratere.";
 50     char    dest1[8];
 51     memmove(dest1, str1, 4);
 52     write(1, dest, 7);
