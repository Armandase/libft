 #include <stdio.h>
 13 #include <string.h>
 14 
 15 int main()
 16 {
 17     char    str[]    = "Premier deuxieme";
 18 
 19     char    str1[]   = "Premier deuxieme";
 20     printf("%s \n", str);
 21 
 22     memmove(str, str + 8, 8 * sizeof(char));
 23     printf("%s \n%s\n", str, (char *)memcpy(str, str + 8, 8 * sizeof(char)));
 24     memmove(str1, str1 + 8, 8 * sizeof(char));
